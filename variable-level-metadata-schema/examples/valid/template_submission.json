{
    "title": "Example VLMD",
    "description": "This is an example description",
    "data_dictionary": [
        {
            "module": "Enrollment",
            "name": "participant_id",
            "title": "Participant Id",
            "description": "Unique identifier for participant",
            "type": "string",
            "constraints": {
                "pattern": "[A-Z][0-9][0-9][0-9]-[0-9][0-9][0-9][0-9]"
            }
        },
        {
            "module": "Demographics",
            "name": "race",
            "title": "Race",
            "description": "Self-reported race",
            "type": "integer",
            "constraints": {
                "enum": [
                    "1",
                    "2",
                    "3",
                    "4",
                    "5",
                    "6",
                    "7",
                    "8"
                ]
            },
            "encodings": {
                "1": "White",
                "2": "Black or African American",
                "3": "American Indian or Alaska Native",
                "4": "Native",
                "5": "Hawaiian or Other Pacific Islander",
                "6": "Asian",
                "7": "Some other race",
                "8": "Multiracial",
                "99": "Not reported"
            },
            "missingValues": [
                "99"
            ],
            "standardsMappings": [
                {
                    "type": "cde",
                    "label": "NLM race",
                    "source": "NLM",
                    "id": "Fakc6Jy2x"
                },
                {
                    "type": "cde",
                    "source": "NLM",
                    "id": "m1_atF7L7U"
                }
            ]
        },
        {
            "module": "Demographics",
            "name": "age",
            "title": "Age",
            "description": "What is your age? (age at enrollment)",
            "type": "integer",
            "constraints": {
                "maximum": 90,
                "minimum": 0
            }
        },
        {
            "module": "Demographics",
            "name": "hispanic",
            "title": "Hispanic, Latino, or Spanish Origin",
            "description": "Are you of Hispanic, Latino, or Spanish origin?",
            "type": "boolean",
            "missingValues": [
                "Not reported"
            ],
            "trueValues": [
                "No"
            ],
            "falseValues": [
                "Yes"
            ]
        },
        {
            "module": "Demographics",
            "name": "sex_at_birth",
            "title": "Sex at Birth",
            "description": "The self-reported sex of the participant/subject at birth",
            "type": "string",
            "constraints": {
                "enum": [
                    "Male",
                    "Female",
                    "Intersex",
                    "None of these describe me",
                    "Prefer not to answer",
                    "Unknown"
                ]
            },
            "missingValues": [
                "Prefer not to answer",
                "Unknown"
            ]
        },
        {
            "module": "Substance Use",
            "name": "SU4",
            "title": "Heroin Days Used",
            "description": "During the past 30 days how many days did you use heroin (alone or mixed with other drugs)? ] [Write 0 days if no use]",
            "type": "integer",
            "relatedConcepts": [
                {
                    "type": "ontology",
                    "url": "https://www.ebi.ac.uk/chebi/chebiOntology.do?chebiId=CHEBI:27808",
                    "source": "CHEBI",
                    "id": "27808"
                },
                {
                    "type": "ontology",
                    "url": "http://purl.bioontology.org/ontology/RXNORM/3304",
                    "source": "RXNORM",
                    "id": "3304"
                }
            ]
        },
        {
            "module": "Biomeasures",
            "name": "pulse_rate",
            "title": "Pulse Rate",
            "description": "Heart rate measured at systemic artery",
            "type": "number",
            "relatedConcepts": [
                {
                    "type": "ontology",
                    "label": "SNOMEDCT bioontology",
                    "url": "http://purl.bioontology.org/ontology/SNOMEDCT/78564009",
                    "source": "SNOMEDCT",
                    "id": "78564009"
                }
            ]
        }
    ]
}